# dot -Tpng visual.dot -o visual.png

digraph visual {

	node [ shape = "record" ]

	Main [ label = "{Main|+ main(String[] args) : String|Lance le jeu}" ];

	Main -> Ariane;
	Main -> IGame;

	Ariane [ label = "{Ariane|}" ];
	IGame [ label = "{Igame||Interface du format type d'un jeu}"];

	edge [ arrowhead="empty" ];
	Ariane -> IGame;
	edge [ arrowhead="normal" ];
}

